        },
        {
            "path": "packages/mcots/tsconfig.json"
        },
        {
            "path": "packages/nps/tsconfig.json"
        },
        {
            "path": "packages/patch/tsconfig.json"
        },
        {
            "path": "packages/persona/tsconfig.json"
        },
        {
            "path": "packages/shard/tsconfig.json"
        },
        {
            "path": "packages/shared/tsconfig.json"
        },
        {
            "path": "packages/shared-packets/tsconfig.json"
        },
        {
            "path": "packages/transactions/tsconfig.json"
        }
    ]
}